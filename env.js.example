// Environment Variables Loader - TEMPLATE
// Copie este arquivo para env.js e preencha com suas credenciais do Firebase
// NUNCA commite o arquivo env.js com credenciais reais!

(function() {
  // Verifica se já existe configuração
  if (window.ENV) return;

  // Configure suas credenciais do Firebase aqui
  window.ENV = {
    FIREBASE_API_KEY: "YOUR_API_KEY_HERE",
    FIREBASE_AUTH_DOMAIN: "YOUR_PROJECT.firebaseapp.com",
    FIREBASE_DATABASE_URL: "https://YOUR_PROJECT.firebaseio.com",
    FIREBASE_PROJECT_ID: "YOUR_PROJECT_ID",
    FIREBASE_STORAGE_BUCKET: "YOUR_PROJECT.appspot.com",
    FIREBASE_MESSAGING_SENDER_ID: "YOUR_SENDER_ID",
    FIREBASE_APP_ID: "YOUR_APP_ID"
  };

  console.log('✅ Environment variables loaded');
})();
